{
  "domain": "tcr",
  "domain_name": "Tribal Climate Resilience",
  "sources": {
    "federal_register": {
      "name": "Federal Register",
      "base_url": "https://www.federalregister.gov/api/v1",
      "requires_key": false,
      "authority_weight": 0.9,
      "search_params": {
        "per_page": 50,
        "order": "newest",
        "conditions[type][]": ["RULE", "PRORULE", "NOTICE", "PRESDOCU"]
      }
    },
    "grants_gov": {
      "name": "Grants.gov",
      "base_url": "https://api.grants.gov/v1",
      "requires_key": false,
      "authority_weight": 0.85,
      "search_params": {
        "rows": 50,
        "sortBy": "openDate|desc"
      }
    },
    "congress_gov": {
      "name": "Congress.gov",
      "base_url": "https://api.congress.gov/v3",
      "requires_key": true,
      "key_env_var": "CONGRESS_API_KEY",
      "authority_weight": 0.8,
      "congress_session": 119,
      "search_params": {
        "limit": 50,
        "sort": "updateDate+desc"
      }
    },
    "usaspending": {
      "name": "USASpending.gov",
      "base_url": "https://api.usaspending.gov/api/v2",
      "requires_key": false,
      "authority_weight": 0.7
    }
  },
  "scoring": {
    "weights": {
      "program_match": 0.35,
      "tribal_keyword_density": 0.20,
      "recency": 0.10,
      "source_authority": 0.15,
      "action_relevance": 0.20
    },
    "critical_boost": 0.15,
    "relevance_threshold": 0.30,
    "tribal_eligibility_override": true
  },
  "tribal_keywords": [
    "tribal", "tribe", "tribes", "indian", "native american",
    "alaska native", "indigenous", "indian country",
    "trust responsibility", "self-determination", "sovereignty",
    "bureau of indian affairs", "BIA", "indian health service",
    "tribal climate", "tribal resilience", "tribal mitigation",
    "tribal set-aside", "tribal consultation", "tribal nations",
    "tribal lands", "reservation", "indian reservation",
    "638 contract", "self-governance", "tribal government",
    "tribal instrumentality", "tribal energy",
    "NEPA", "environmental justice", "treaty rights", "treaty obligation",
    "EO 13175", "government-to-government", "nation-to-nation",
    "allotment", "fee-to-trust", "ISDEAA", "P.L. 93-638",
    "trust land", "tribal college", "tribal utility",
    "alaska native corporation", "tribal enterprise", "compact",
    "direct service tribe"
  ],
  "action_keywords": [
    "notice of funding", "NOFO", "request for applications",
    "proposed rule", "final rule", "executive order",
    "appropriation", "authorization", "reauthorization",
    "amendment", "extension", "deadline", "comment period",
    "grant opportunity", "funding opportunity", "solicitation",
    "budget request", "continuing resolution", "omnibus",
    "rescission", "impoundment", "termination", "replacement",
    "information collection request", "paperwork reduction",
    "request for information", "advance notice of proposed rulemaking",
    "significant guidance", "policy statement", "memorandum",
    "dear tribal leader", "tribal consultation notice",
    "consent decree", "settlement agreement",
    "interagency agreement", "memorandum of understanding",
    "reconciliation", "reconciliation bill", "budget reconciliation",
    "revenue offsets", "repeal", "section 6417"
  ],
  "search_queries": [
    "tribal climate resilience",
    "tribal climate adaptation",
    "tribal hazard mitigation",
    "indian tribe grant",
    "tribal energy",
    "tribal infrastructure",
    "BRIC tribal",
    "elective pay tribal",
    "tribal clean energy",
    "tribal water infrastructure",
    "tribal wildfire",
    "tribal housing resilience",
    "budget reconciliation tribal",
    "IRA repeal tribal",
    "reconciliation bill energy",
    "section 6417 repeal",
    "tribal NEPA", "tribal environmental justice",
    "IHBG tribal housing", "tribal air quality",
    "NOAA tribal climate", "WaterSMART tribal",
    "tribal transportation program"
  ],
  "scan_window_days": 14,
  "monitors": {
    "iija_sunset": {
      "warning_days": 180,
      "critical_days": 90,
      "fy26_end": "2026-09-30"
    },
    "reconciliation": {
      "keywords": ["reconciliation", "repeal", "section 6417", "elective pay", "IRA repeal", "direct pay termination"],
      "active_bill_statuses": ["introduced", "committee", "floor", "conference"],
      "enacted_laws_exclude": ["Public Law 119-21"]
    },
    "tribal_consultation": {
      "keywords": ["dear tribal leader", "DTLL", "tribal consultation", "EO 13175", "Executive Order 13175", "consultation notice", "government-to-government", "nation-to-nation"],
      "agency_slugs": ["indian-affairs-bureau", "environmental-protection-agency", "interior-department", "energy-department"]
    },
    "hot_sheets": {
      "staleness_days": 90
    },
    "dhs_funding": {
      "cr_expiration": "2026-02-13",
      "fema_program_ids": ["fema_bric", "fema_tribal_mitigation"],
      "warning_days": 14
    },
    "decision_engine": {
      "urgency_threshold_days": 30
    }
  },
  "packets": {
    "tribal_registry": {
      "data_path": "data/tribal_registry.json"
    },
    "congressional_cache": {
      "data_path": "data/congressional_cache.json"
    },
    "ecoregion": {
      "data_path": "data/ecoregion_config.json"
    },
    "output_dir": "outputs/packets",
    "awards": {
      "alias_path": "data/tribal_aliases.json",
      "cache_dir": "data/award_cache",
      "fuzzy_threshold": 85
    },
    "hazards": {
      "nri_dir": "data/nri",
      "usfs_dir": "data/usfs",
      "cache_dir": "data/hazard_profiles",
      "crosswalk_path": "data/aiannh_tribe_crosswalk.json"
    }
  }
}
